<?xml version="1.0" encoding="utf-8"?>
<config>
	<honcheonui>
		<name>honcheonui</name>
		<version>0.1</version>
		<pid_file>/tmp/honcheonui-agent.pid</pid_file>
		<loglevel>debug</loglevel>
	</honcheonui>
	<master>
		<host>localhost</host>
		<port>3000</port>
	</master>
	<server>
		<uuid></uuid>
		<asset_id></asset_id>
	</server>
	<mod_status>
		<path>/status.json</path>
	</mod_status>
	<mod_log>
		<group>messages</group>
		<path>/logs.json</path>
		<pipe>/tmp/log.pipe</pipe>
	</mod_log>
</config>
