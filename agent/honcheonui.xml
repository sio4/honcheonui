<?xml version='1.0' encoding='utf-8'?>
<config>
	<honcheonui>
		<name>honcheonui</name>
		<version>0.1</version>
		<pid_file>/tmp/honcheonui-agent.pid</pid_file>
		<loglevel>debug</loglevel>
	</honcheonui>
	<master>
		<host>localhost</host>
		<port>3000</port>
	</master>
	<module>
		<server type='startup'>
			<uuid></uuid>
			<asset_id></asset_id>
			<path>/server.json</path>
		</server>
		<ds_local type='storage'>
			<file>/tmp/localstore</file>
		</ds_local>
		<stat type='periodic'>
			<path>/stats.json</path>
		</stat>
		<logger type='periodic'>
			<group>messages</group>
			<path>/logs.json</path>
			<pipe>/tmp/log.pipe</pipe>
		</logger>
	</module>
</config>
