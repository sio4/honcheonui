<h1>Listing wikis</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Source</th>
    <th>Wiki</th>
    <th>Version</th>
    <th>Topic</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wikis.each do |wiki| %>
  <tr>
    <td><%= wiki.name %></td>
    <td><%= wiki.source %></td>
    <td><%= wiki.wiki %></td>
    <td><%= wiki.version %></td>
    <td><%= wiki.topic %></td>
    <td><%= wiki.user.name %></td>
    <td><%= link_to 'Show', wiki %></td>
    <td><%= link_to 'Edit', edit_wiki_path(wiki) %></td>
    <td><%= link_to 'Destroy', wiki, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wiki', new_wiki_path %>
